<?xml version="1.0" encoding="UTF-8"?>
<category
	xmlns="http://app.kitchenassistant.com.pl/categoriesSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://app.kitchenassistant.com.pl/categoriesSchema categoriesSchema.xsd"
	name="root">

	<conditions>
	</conditions>
	<branchConditions>
	</branchConditions>


	<children>
		<category name="raw">
			<conditions>
				<condition>
					<departmentName contains="fresh" />

					 <departmentName contains="food" /> 
				</condition>
			</conditions>

			<branchConditions>
			</branchConditions>

			<children>
				<category name="package">
					<conditions></conditions>

					<branchConditions>
					</branchConditions>
					<children>
					</children>
				</category>

			</children>


		</category>
		<category name="processed">
			<conditions>
				<condition>
					<name contains="processed" />
				</condition>
			</conditions>
			<branchConditions>
			</branchConditions>
			<children>
				<category name="dried">
					<conditions>

						<condition>
							<name contains="dried" />
						</condition>
						<condition>
							<name contains="powder" />
						</condition>
					</conditions>
					<children></children>
					<branchConditions>
					</branchConditions>

				</category>
				<category name="grinded">
					<conditions>

						<condition>
							<name contains="paste" />
						</condition>
						<condition>
							<name contains="puree" />
						</condition>
					</conditions>

					<children></children>
					<branchConditions>
					</branchConditions>

				</category>
				<category name="compoundDish">
					<conditions>

					</conditions>
					<children></children>
					<branchConditions>
					</branchConditions>

				</category>
				<category name="conserved">
					<conditions>
						<condition>
							<name contains="pickles" />
						</condition>
						<condition>
							<attributes contains="drainedWeight" />
						</condition>
						<condition>
							<departmentName contains="cans" />
							<name notContains="puree" />

						</condition></conditions>

					<branchConditions>
					</branchConditions>
					<children></children>
				</category>

			</children>

		</category>


		<category name="residual">
			<conditions>
				<condition>
					<name contains="flavoured" />
				</condition>
				<condition>
					<departmentName contains="drinks" />
				</condition>
				<condition>
					<departmentName contains="household" />
				</condition>
				<condition>
					<departmentName contains="beauty" />
				</condition>
			</conditions>
			<branchConditions>
			</branchConditions>
			<children>
			</children>
		</category>

	</children>
</category>
