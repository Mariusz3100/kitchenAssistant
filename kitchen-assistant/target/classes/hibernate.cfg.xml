<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-configuration PUBLIC
        "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
	<session-factory>
		<property name="dialect">org.hibernate.dialect.PostgreSQLDialect</property>
		<property name="show_sql">false</property>
		<mapping resource="mariusz/ambroziak/kassistant/model/User.hbm.xml" />
		<mapping class="mariusz.ambroziak.kassistant.model.Produkt" />
		<mapping class="mariusz.ambroziak.kassistant.model.Base_Word" />
		<mapping class="mariusz.ambroziak.kassistant.model.Variant_Word" />
		<mapping class="mariusz.ambroziak.kassistant.model.Amount_Type" />
		<mapping class="mariusz.ambroziak.kassistant.model.Recipe_Ingredient" />
		<mapping class="mariusz.ambroziak.kassistant.model.Recipe" />
		<mapping class="mariusz.ambroziak.kassistant.model.Problem" />
		<mapping class="mariusz.ambroziak.kassistant.model.Basic_Ingredient" />
		<mapping class="mariusz.ambroziak.kassistant.model.Basic_Ingredient_Name" />
		<mapping class="mariusz.ambroziak.kassistant.model.Nutrient" />
		<mapping class="mariusz.ambroziak.kassistant.model.Basic_Ingredient_Nutrient_amount" />
		
	</session-factory>
</hibernate-configuration>